[Unit]
Description=Gunicorn server for DOMAIN

[Service]
Restart=on-failure
User=koyangi
WorkingDirectory=/home/koyangi/python/tdd-book
EnviornmentFile=/home/koyangi/python/tdd-book/.env

ExecStart=/home/koyangi/python/tdd-book/virtualenv/bin/gunicorn \
--bind unix:/tmp/DOMAIN/socket \
superlists.wsgi:application

[Install]
WantedBy=multi-user.target
